{
  "name": "geovistory",
  "scripts": {  
    "postdeploy": "bash heroku-prebuild.sh && bash heroku-postbuild.sh"
  },
  "env": {
    "DB_ENV": "review",
    "GEOV_STAG_DATABASE_URL": {
      "required": true
    },
    "HEROKU_APP_NAME": {
      "required": true
    }
  },
  "formation": {
  },
  "addons": [
    {
      "plan": "heroku-postgresql:hobby-basic",
      "options": {
        "version": "10"
      }
    }
  ],
  "buildpacks": [
    {
      "url": "heroku/python"
    },
    {
      "url": "heroku/nodejs"
    }
  ]
}