{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "defaultProject": "toolbox",
  "projects": {
    "toolbox": {
      "root": "projects/toolbox",
      "sourceRoot": "projects/toolbox/src",
      "projectType": "application",
      "prefix": "gv",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/toolbox",
            "index": "projects/toolbox/src/index.html",
            "main": "projects/toolbox/src/main.ts",
            "polyfills": "projects/toolbox/src/polyfills.ts",
            "tsConfig": "projects/toolbox/tsconfig.app.json",
            "assets": [
              "projects/toolbox/src/assets",
              "projects/toolbox/src/favicon.ico",
              {
                "glob": "**/*",
                "input": "node_modules/cesium/Build/Cesium",
                "output": "./assets/cesium"
              },
              {
                "glob": "mdi.svg",
                "input": "node_modules/@mdi/angular-material",
                "output": "./assets/mdi"
              }
            ],
            "styles": [
              "projects/toolbox/src/styles.scss",
              "geovistory-font/dist/Geovistory.css",
              "node_modules/font-awesome/css/font-awesome.css",
              "node_modules/quill/dist/quill.bubble.css",
              "node_modules/quill/dist/quill.snow.css",
              "node_modules/@cime/ngx-toasty/style-bootstrap.css",
              "node_modules/cesium/Build/Cesium/Widgets/widgets.css",
              "node_modules/primeng/resources/primeng.min.css"
            ],
            "scripts": [
              "node_modules/cesium/Build/Cesium/Cesium.js"
            ],
            "webWorkerTsConfig": "projects/toolbox/tsconfig.worker.json"
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "fileReplacements": [
                {
                  "replace": "projects/toolbox/src/environments/environment.ts",
                  "with": "projects/toolbox/src/environments/environment.prod.ts"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "toolbox:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "toolbox:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "toolbox:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/toolbox/src/test.ts",
            "polyfills": "projects/toolbox/src/polyfills.ts",
            "tsConfig": "projects/toolbox/tsconfig.spec.json",
            "karmaConfig": "projects/toolbox/karma.conf.js",
            "sourceMap": {
              "scripts": false,
              "styles": false,
              "vendor": false
            },
            "scripts": [
              "node_modules/cesium/Build/Cesium/Cesium.js"
            ],
            "styles": [
              "projects/toolbox/src/styles.scss",
              "geovistory-font/dist/Geovistory.css",
              "node_modules/font-awesome/css/font-awesome.css",
              "node_modules/quill/dist/quill.bubble.css",
              "node_modules/quill/dist/quill.snow.css",
              "node_modules/@cime/ngx-toasty/style-bootstrap.css",
              "node_modules/cesium/Build/Cesium/Widgets/widgets.css",
              "node_modules/primeng/resources/primeng.min.css"
            ],
            "assets": [
              "projects/toolbox/src/assets",
              "projects/toolbox/src/favicon.ico",
              {
                "glob": "**/*",
                "input": "node_modules/cesium/Build/Cesium",
                "output": "./assets/cesium"
              }
            ]
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "projects/toolbox/tsconfig.app.json",
              "projects/toolbox/tsconfig.spec.json",
              "projects/toolbox/e2e/tsconfig.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      },
      "schematics": {
        "@schematics/angular:component": {
          "prefix": "gv",
          "styleext": "scss"
        },
        "@schematics/angular:directive": {
          "prefix": "gv"
        }
      }
    },
    "playground": {
      "root": "",
      "sourceRoot": "src",
      "projectType": "application",
      "architect": {
        "test": {
          "scripts": [
            "./node_modules/cesium/Build/Cesium/Cesium.js"
          ]
        },
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/playground",
            "index": "src/index.html",
            "main": "src/main.playground.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "src/tsconfig.playground.json",
            "assets": [
              "src/assets",
              "src/favicon.ico",
              {
                "glob": "**/*",
                "input": "node_modules/cesium/Build/Cesium",
                "output": "./assets/cesium"
              },
              {
                "glob": "mdi.svg",
                "input": "node_modules/@mdi/angular-material",
                "output": "./assets/mdi"
              }
            ],
            "styles": [
              "projects/toolbox/src/styles.scss",
              "geovistory-font/dist/Geovistory.css",
              "node_modules/font-awesome/css/font-awesome.css",
              "node_modules/quill/dist/quill.snow.css",
              "node_modules/quill/dist/quill.bubble.css",
              "node_modules/cesium/Build/Cesium/Widgets/widgets.css"
            ],
            "scripts": [
              "node_modules/cesium/Build/CesiumUnminified/Cesium.js"
            ]
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": false
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "playground:build",
            "port": 4201
          }
        }
      }
    }
  }
}
