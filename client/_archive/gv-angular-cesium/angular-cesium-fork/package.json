{
  "name": "angular-cesium",
  "version": "0.0.52",
  "license": "MIT",
  "description": "Angular library for working with Cesium.",
  "repository": "https://github.com/TGFTech/angular-cesium",
  "keywords": [
    "Angular2",
    "Cesium",
    "Angular Cesium",
    "Angular2 Cesium"
  ],
  "homepage": "https://github.com/TGFTech/angular-cesium#readme",
  "main": "bundles/angular-cesium.umd.js",
  "typings": "index.d.ts",
  "engineStrict": false,
  "engines": {
    "node": ">= 7.9.0"
  },
  "scripts": {
    "clean_src": "rimraf src/**/*.metadata.json src/**/*.js src/**/*.js.map src/**/*.ngfactory.ts src/**/*.ngstyle.ts src/**/*.ngsummary.json",
    "clean_typings": "rimraf src/*.d.ts src/angular-cesium/**/*.d.ts src/angular-cesium-entities-editor/**/*.d.ts src/heatmap/**/*.d.ts src/cesium-extender/**/*.d.ts src/angular-cesium-widgets/**/*.d.ts",
    "cleanup": "npm run clean_src && npm run clean_typings && rimraf index.js index.js.map index.d.ts index.metadata.json build bundles",
    "start": "webpack-dev-server --inline --progress --port 8081",
    "lint": "tslint -c tslint.json --project tsconfig.aot.json",
    "test": "karma start",
    "ngc": "ngc -p tsconfig.aot.json",
    "aot": "webpack --config webpack.aot.config.js -p",
    "rollup": "rollup -cza",
    "bundle": "npm run cleanup && npm run lint && npm run ngc && npm run rollup",
    "build-demo": "rimraf dist && webpack --config config/webpack.dev.js --progress --profile --bail",
    "test-server": "node demo/performance-server/server.js",
    "server": "nodemon --exec ts-node -- demo/server/src/main.ts",
    "docs": "compodoc -p src/tsconfig.compodoc.json -d docs -n \"Angular 2 Cesium\" --watch --serve",
    "compile-docs": "rimraf docs && compodoc -p src/tsconfig.compodoc.json -d docs -n \"Angular 2 Cesium\"",
    "gh-pages": "gh-pages -d docs -b gh-pages",
    "docs:push": "npm run compile-docs && npm run gh-pages && rimraf docs",
    "heroku-postbuild": "npm run build-demo",
    "prepublishOnly": "npm run bundle",
    "postpublish": "npm run docs:push",
    "pack": "npm run bundle && node_modules/typescript/bin/tsc && npm pack"
  },
  "peerDependencies": {
    "@angular/common": "^4.2.6 || ^5.0.0",
    "@angular/core": "^4.2.6 || ^5.0.0"
  },
  "devDependencies": {
    "@angular/animations": "5.1.0",
    "@angular/cdk": "5.0.1",
