{
  "compileOnSave": false,
  "angularCompilerOptions": {
    "strictTemplates": true,
    "fullTemplateTypeCheck": true
  },
  "compilerOptions": {
    "baseUrl": "./",
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "downlevelIteration": true,
    "esModuleInterop": true,
    "experimentalDecorators": true,
    "moduleResolution": "node",
    "importHelpers": true,
    "target": "es2022",
    "module": "es2022",
    "allowSyntheticDefaultImports": true,
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "types": [
      "socket.io-client",
      "jasmine"
    ],
    "typeRoots": [
      "node_modules/@types"
    ],
    "lib": [
      "es2016",
      "dom"
    ],
    "paths": {
      "@kleiolab/lib-sdk-lb4/*": [
        "dist/lib-sdk-lb4/*"
      ],
      "@kleiolab/lib-sdk-lb4": [
        "dist/lib-sdk-lb4"
      ],
      "@kleiolab/lib-utils/*": [
        "dist/lib-utils/*"
      ],
      "@kleiolab/lib-utils": [
        "dist/lib-utils"
      ],
      "@kleiolab/lib-config/*": [
        "dist/lib-config/*"
      ],
      "@kleiolab/lib-config": [
        "dist/lib-config"
      ],
      "@kleiolab/lib-sockets": [
        "dist/lib-sockets"
      ],
      "@kleiolab/lib-sockets/*": [
        "dist/lib-sockets/*"
      ],
      "@kleiolab/lib-redux/*": [
        "dist/lib-redux/*"
      ],
      "@kleiolab/lib-redux": [
        "dist/lib-redux"
      ],
      "@kleiolab/lib-queries": [
        "dist/lib-queries"
      ],
      "@kleiolab/lib-queries/*": [
        "dist/lib-queries/*"
      ],
      "util": [
        "./util-polyfill.js"
      ]
    }
  },
  "exclude": [
    "node_modules"
  ]
}
