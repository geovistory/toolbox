{
  "name": "geovistory",
  "version": "1.0.0",
  "description": "Geovistory â€“ Platform for Digital History",
  "keywords": [
    "loopback-application",
    "loopback"
  ],
  "main": "index.js",
  "engines": {
    "node": ">=10",
    "npm": "6.7.0"
  },
  "cacheDirectories": [
    "node_modules"
  ],
  "scripts": {
    "build": "lb-tsc",
    "build:watch": "lb-tsc --watch",
    "lint": "npm run prettier:check && npm run eslint",
    "lint:fix": "npm run eslint:fix && npm run prettier:fix",
    "prettier:cli": "lb-prettier \"**/*.ts\" \"**/*.js\"",
    "prettier:check": "npm run prettier:cli -- -l",
    "prettier:fix": "npm run prettier:cli -- --write",
    "eslint": "lb-eslint --report-unused-disable-directives .",
    "eslint:fix": "npm run eslint -- --fix",
    "pretest": "npm run clean && npm run build",
    "test": "lb-mocha --allow-console-logs \"dist/__tests__\"",
    "posttest": "npm run lint",
    "test:dev": "lb-mocha --timeout 5000 --allow-console-logs dist/__tests__/**/*.js && npm run posttest",
    "migrate": "node ./dist/migrate",
    "prestart": "npm run build",
    "start": "node -r source-map-support/register .",
    "clean": "lb-clean dist *.tsbuildinfo .eslintcache",
    "heroku-postbuild": "bash heroku-postbuild.sh",
    "t": "export NO_LOGS='true' && lb-mocha --timeout 4000 dist/__tests__/**/*.js"
  },
  "repository": {
    "type": "git"
  },
  "author": "",
  "license": "",
  "files": [
    "README.md",
    "index.js",
    "index.d.ts",
    "dist",
    "src",
    "!*/__tests__"
  ],
  "dependencies": {
    "@loopback/boot": "^2.2.0",
    "@loopback/booter-lb3app": "^2.1.3",
    "@loopback/context": "^3.7.0",
    "@loopback/core": "^2.5.0",
    "@loopback/openapi-v3": "^3.3.1",
    "@loopback/repository": "^2.4.0",
    "@loopback/rest": "^4.0.0",
    "@loopback/rest-explorer": "^2.2.0",
    "@loopback/service-proxy": "^2.2.0",
    "cli-color": "^1.2.0",
    "compression": "^1.0.3",
    "cors": "^2.5.2",
    "express-sslify": "^1.2.0",
    "forever": "^0.15.3",
    "helmet": "^1.3.0",
    "json2csv": "^4.3.5",
    "lodash": "^4.17.15",
    "loopback": "^3.26.0",
    "loopback-boot": "^2.6.5",
    "loopback-component-explorer": "^4.0.0",
    "loopback-connector-postgresql": "^2.8.0",
    "loopback-postgres-find-complex": "github:kleiolab/findComplex#bug-fixes",
    "loopback-setup-remote-methods-mixin": "^1.2.1",
    "pg": "^7.4.1",
    "ramda": "^0.26.1",
    "rxjs": "^6.5.3",
    "socket.io": "^2.2.0",
    "sql-formatter": "^2.3.2",
    "tracer": "^0.8.11",
    "tslib": "^1.10.0"
  },
  "devDependencies": {
    "@loopback/build": "^5.3.1",
    "@loopback/eslint-config": "^6.0.6",
    "@loopback/testlab": "^3.1.3",
    "@types/json2csv": "^4.5.0",
    "@types/node": "^10.17.21",
    "@types/pg": "^7.14.1",
    "@types/ramda": "^0.26.33",
    "@types/socket.io": "^2.1.2",
    "@types/socket.io-client": "^1.4.33",
    "@types/sql-formatter": "^2.3.0",
    "@typescript-eslint/eslint-plugin": "^2.31.0",
    "@typescript-eslint/parser": "^2.31.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-eslint-plugin": "^2.2.1",
    "eslint-plugin-mocha": "^6.3.0",
    "json-schema-to-typescript": "^7.1.0",
    "nodemon": "^2.0.3",
    "pascalcase": "^1.0.0",
    "socket.io-client": "^2.3.0",
    "source-map-support": "^0.5.19",
    "typescript": "~3.8.3",
    "typescript-json-schema": "^0.40.0"
  }
}
