{
  "name": "PubAccount",
  "base": "PersistedModel",
  "idInjection": true,
  "postgresql": {
    "schema": "public",
    "table": "account"
  },
  "mixins": {
    "SetupRemoteMethods": {
      "ignoreACL": false,
      "disableAllExcept": []
    }
  },
  "properties": {
    "id": {
      "type": "number"
    },
    "realm": {
      "type": "string"
    },
    "username": {
      "type": "string"
    },
    "email": {
      "type": "string",
      "required": true
    },
    "emailVerified": "boolean",
    "verificationToken": "string"
  },
  "options": {
    "caseSensitiveEmail": true
  },
  "hidden": [
    "verificationToken"
  ],
  "validations": [],
  "relations": {
    "projects": {
      "type": "hasMany",
      "model": "ProProject",
      "foreignKey": "account_id",
      "keyThrough": "fk_project",
      "through": "PubAccountProjectRel"
    },
    "accessTokens": {
      "type": "hasMany",
      "model": "AccessToken",
      "foreignKey": "userId",
      "options": {
        "disableInclude": true
      }
    }
  },
  "acls": [
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "DENY"
    },
    {
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "ALLOW",
      "property": [
        "login",
        "logout"
      ]
    },
    {
      "principalType": "ROLE",
      "principalId": "$authenticated",
      "permission": "ALLOW",
      "property": []
    },
    {
      "accessType": "EXECUTE",
      "principalType": "ROLE",
      "principalId": "system_admin",
      "permission": "ALLOW",
      "property": [
        "withRolesAndProjects"
      ]
    }
  ],
  "methods": {}
}
