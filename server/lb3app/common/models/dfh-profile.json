{
  "name": "DfhProfile",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true,
    "postgresql": {
      "schema": "data_for_history",
      "table": "v_profile"
    }
  },
  "mixins": {
    "SetupRemoteMethods": {
      "ignoreACL": false,
      "disableAllExcept": []
    }
  },
  "properties": {
    "pk_profile": {
      "id": true,
      "type": "number"
    },
    "owned_by_project": {
      "type": "number"
    },
    "is_ongoing_forced_publication": {
      "type": "boolean"
    },
    "date_profile_published": {
      "type": "string"
    },
    "date_profile_deprecated": {
      "type": "string"
    },
    "tmsp_last_dfh_update": {
      "type": "string"
    }
  },
  "validations": [],
  "relations": {},
  "acls": [
    {
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "DENY",
      "property": "*"
    },
    {
      "principalType": "ROLE",
      "principalId": "$authenticated",
      "permission": "ALLOW",
      "property": [
      ]
    },
    {
      "principalType": "ROLE",
      "principalId": "project_member",
      "permission": "ALLOW",
      "property": [
        "ofProject"
      ]
    }
  ],
  "methods": {
    "ofProject": {
      "accepts": [
        {
          "arg": "pkProject",
          "type": "number",
          "required": false,
          "description": "Project pk",
          "http": {
            "source": "query"
          }
        }
      ],
      "returns": {
        "arg": "data",
        "type": [
          "DfhProfile"
        ],
        "root": true
      },
      "description": "Get all profiles that are used by the given project.",
      "http": {
        "path": "/of-project",
        "verb": "get"
      }
    }
  }
}
