{ // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Launch App",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "program": "${workspaceFolder}/index.js",
      "preLaunchTask": "tsc: watch - tsconfig.json",
      "runtimeExecutable": null,
      "envFile": "${workspaceFolder}/.vscode/envFile.dev",
      "restart": true,
      "outFiles": [
        "${workspaceFolder}/dist/**/*.js"
      ]
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Launch App:watch",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "program": "${workspaceFolder}/index.js",
      "preLaunchTask": "tsc: watch - tsconfig.json",
      "runtimeExecutable": "${workspaceFolder}/node_modules/.bin/nodemon",
      "envFile": "${workspaceFolder}/.vscode/envFile.dev",
      "restart": true,
      "outFiles": [
        "${workspaceFolder}/dist/**/*.js"
      ]
    },
    {
      "name": "Launch Tests",
      "type": "node",
      "request": "launch",
      "preLaunchTask": "tsc: watch - tsconfig.json",
      "program": "${workspaceRoot}/node_modules/mocha/bin/_mocha",
      "stopOnEntry": false,
      "args": [
        "--timeout",
        "20000",
        "--colors",
        "'dist/__tests__/**/*.js'",
        // "--grep",
        // "/PingController.*/"
      ],
      "cwd": "${workspaceRoot}",
      "runtimeExecutable": null,
      "envFile": "${workspaceFolder}/.vscode/envFile.test",
      "outFiles": [
        "${workspaceFolder}/dist/**/*.js"
      ]
    },
    {
      "name": "Launch Tests:watch",
      "type": "node",
      "request": "launch",
      "preLaunchTask": "tsc: watch - tsconfig.json",
      "program": "${workspaceRoot}/node_modules/.bin/_mocha",
      "stopOnEntry": false,
      "args": [
        "--timeout",
        "20000",
        "--colors",
        "'dist/__tests__/**/*.js'",
        // "--grep",
        // "/WarEntityPreviewController.*/"
      ],
      "cwd": "${workspaceRoot}",
      "runtimeExecutable": "${workspaceFolder}/node_modules/.bin/nodemon",
      "restart": true,
      "envFile": "${workspaceFolder}/.vscode/envFile.test",
      "outFiles": [
        "${workspaceFolder}/dist/**/*.js"
      ]
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Launch Warehouse",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "program": "${workspaceFolder}/warehouse.js",
      "preLaunchTask": "tsc: watch - tsconfig.json",
      "runtimeExecutable": "${workspaceFolder}/node_modules/.bin/nodemon",
      "restart": true,
      "outputCapture": "std",
      "envFile": "${workspaceFolder}/.vscode/envFile.dev",
      "outFiles": [
        "${workspaceFolder}/dist/**/*.js"
      ]
    },
    {
      "name": "Launch Warehouse Tests",
      "type": "node",
      "request": "launch",
      "preLaunchTask": "tsc: watch - tsconfig.json",
      "program": "${workspaceRoot}/node_modules/.bin/mocha",
      "stopOnEntry": false,
      "args": [
        "--timeout",
        "5000",
        "--colors",
        "'dist/__tests__/**/warehouse/**/*.js'",
        "--grep",
        "/PEntityTypeService/"
      ],
      "cwd": "${workspaceRoot}",
      "restart": false,
      "envFile": "${workspaceFolder}/.vscode/envFile.test",
      "outFiles": [
        "${workspaceFolder}/dist/**/*.js"
      ]
    },
  ]
}
