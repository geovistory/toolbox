{
  "name": "geovistory",
  "version": "1.0.0",
  "main": "server/server.js",
  "engines": {
    "node": "8.9.4",
    "npm": "6.7.0"
  },
  "scripts": {
    "lint": "eslint .",
    "heroku-postbuild": "bash heroku-postbuild.sh",
    "push": "bash push.sh",
    "start": "node .",
    "create-schemas": "node common/src/generate-schema-and-validator.js",
    "build": "tsc -b -v",
    "clean": "tsc -b --clean",
    "posttest": "npm run lint && nsp check",
    "build:sdk": "./node_modules/.bin/lb-sdk server/server client/src/app/core/sdk -d ng2web -l angular2",
    "db-migrate": "node_modules/db-migrate/bin/db-migrate --config server/migrate-db-config.json --migrations-dir server/migrations",
    "cm": "node_modules/db-migrate/bin/db-migrate --config server/migrate-db-config.json --migrations-dir server/migrations --sql-file true create",
    "mu": "node_modules/db-migrate/bin/db-migrate --config server/migrate-db-config.json --migrations-dir server/migrations up",
    "md": "node_modules/db-migrate/bin/db-migrate --config server/migrate-db-config.json --migrations-dir server/migrations down"
  },
  "dependencies": {
    "@mean-expert/loopback-sdk-builder": "kleiolab/loopback-sdk-builder#fix-missing-targetClass",
    "@mean-expert/model": "^1.0.9",
    "@types/socket.io": "^2.1.2",
    "JSONStream": "^1.3.5",
    "ajv": "^6.10.2",
    "bluebird": "^3.5.1",
    "cli-color": "^1.2.0",
    "cli-progress": "^1.8.0",
    "compression": "^1.0.3",
    "cors": "^2.5.2",
    "csv-write-stream": "^2.0.0",
    "db-migrate": "^0.11.5",
    "db-migrate-pg": "^0.4.0",
    "express-sslify": "^1.2.0",
    "faker": "^4.1.0",
    "forever": "^0.15.3",
    "fs-extra": "^8.0.1",
    "helmet": "^1.3.0",
    "iso-639-3": "^1.0.1",
    "json2csv": "^4.3.5",
    "lodash": "^4.17.15",
    "loopback": "^3.26.0",
    "loopback-boot": "^2.6.5",
    "loopback-component-explorer": "^4.0.0",
    "loopback-connector-postgresql": "^2.8.0",
    "loopback-postgres-find-complex": "github:kleiolab/findComplex#bug-fixes",
    "loopback-setup-remote-methods-mixin": "^1.2.1",
    "pg": "^7.4.1",
    "pg-query-stream": "^1.1.2",
    "python-shell": "^0.5.0",
    "ramda": "^0.26.1",
    "rxjs": "^6.5.3",
    "serve-favicon": "^2.0.1",
    "socket.io": "^2.2.0",
    "sql-formatter": "^2.3.2",
    "strong-error-handler": "^2.0.0",
    "tracer": "^0.8.11"
  },
  "devDependencies": {
    "@types/ajv": "^1.0.0",
    "@types/node": "^12.12.6",
    "@types/ramda": "^0.26.33",
    "@types/sql-formatter": "^2.3.0",
    "eslint": "^3.17.1",
    "eslint-config-loopback": "^8.0.0",
    "json-schema-to-typescript": "^7.1.0",
    "nodemon": "^1.17.5",
    "nsp": "^2.1.0",
    "pascalcase": "^1.0.0",
    "typescript": "^3.4.5",
    "typescript-json-schema": "^0.40.0"
  },
  "repository": {
    "type": "",
    "url": ""
  },
  "cacheDirectories": [
    "client/node_modules",
    "node_modules"
  ],
  "license": "UNLICENSED",
  "description": "geovistory"
}
