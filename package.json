{
  "name": "geovistory-root",
  "description": "package.json for DevOps scripts",
  "version": "0.0.0",
  "scripts": {
    "docker": "node ./server/scripts/docker.js",
    "loopack-build:watch": "cd server && npm run build:watch",
    "install": "cd ./server && npm i --production=false",
    "heroku-postbuild": "bash heroku-postbuild.sh"
  },
  "private": true,
  "main": "index.js",
  "engines": {
    "node": "12.8.1",
    "npm": "6.13.2"
  },
  "cacheDirectories": [
    "server/node_modules",
    "client/node_modules"
  ]
}
