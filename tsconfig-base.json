{
  "compilerOptions": {
    "declaration": true,
    "target": "es2015",
    "module": "commonjs",
    "strict": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true,
    "composite": true,
    "resolveJsonModule": true,
  }
}
