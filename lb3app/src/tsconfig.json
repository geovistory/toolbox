{
  "compilerOptions": {
    "outDir": "../dist",
    "rootDir": ".",
    "declaration": false,
    "target": "es2015",
    "module": "commonjs",
    "strict": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true,
    "moduleResolution": "node",
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "sourceMap": true,
    "types": [],
    "typeRoots": [
      "../node_modules/@types"
    ],
  },
  "exclude": [
    "../node_modules",
    "**/*.d.ts",
    "**/typegen.ts"
  ]
}
