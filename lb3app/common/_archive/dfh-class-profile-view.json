      "type": "number",
      "id": true,
      "generated": true
    },
    "is_enabled_in_profile": {
      "type": "boolean"
    },
    "removed_from_api": {
      "type": "boolean"
    },
    "dfh_fk_class": {
      "type": "number"
    },
    "dfh_fk_system_type": {
      "type": "number"
    },
    "dfh_type_label": {
      "type": "string"
    },
    "dfh_root_namespace": {
      "type": "string"
    },
    "dfh_profile_association_type": {
      "type": "string"
    },
    "dfh_fk_profile": {
      "type": "number"
    },
    "dfh_profile_label": {
      "type": "string"
    }
  },
  "validations": [],
  "relations": {
