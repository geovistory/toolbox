{
  "compileOnSave": true,
  "compilerOptions": {
    "rootDir": ".",
    "outDir": "./dist",
    "baseUrl": "./src",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "target": "ES6",
    "types": [],
    "typeRoots": [
      "../../node_modules/@types"
    ],
    "module": "commonjs",
    "declaration": true,
    "declarationMap": true,
    "composite": true,
  },
  "references": [],
  "exclude": [
    "../../node_modules",
    "**/*.d.ts",
    "**/typegen.ts"
  ]
}
