{
  "name": "ProAnalysis",
  "base": "ProEntity",
  "idInjection": false,
  "options": {
    "validateUpsert": true,
    "postgresql": {
      "schema": "projects",
      "table": "analysis"
    }
  },
  "properties": {
    "name": {
      "type": "string",
      "required": true
    },
    "description": {
      "type": "string",
      "required": false
    },
    "analysis_definition": {
      "type": "object",
      "required": true
    },
    "fk_project": {
      "type": "number",
      "required": true
    },
    "fk_analysis_type": {
      "type": "number",
      "required": true
    }
  },
  "validations": [],
  "relations": {
    "project": {
      "type": "belongsTo",
      "model": "ProProject",
      "foreignKey": "fk_project",
      "primaryKey": "pk_entity"
    },
    "account": {
      "type": "belongsTo",
      "model": "PubAccount",
      "foreignKey": "fk_last_modifier",
      "primaryKey": "id"
    }
  },
  "acls": [
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "DENY"
    },
    {
      "principalType": "ROLE",
      "principalId": "project_member",
      "permission": "ALLOW",
      "property": [
        "run",
        "runAndExport",
        "bulkUpsert",
        "bulkDelete",
        "findPerIdAndVersionAndProject"
      ]
    }
  ],
  "methods": {
    "run": {
      "accepts": [
        {
          "arg": "pkProject",
          "type": "number",
          "required": true,
          "description": "Pk of the project.",
          "http": {
            "source": "query"
          }
        },
        {
          "arg": "analysisType",
          "type": "number",
          "required": true,
          "description": "Pk of the analysis type.",
          "http": {
            "source": "query"
          }
        },
        {
          "arg": "analysisDefinition",
          "type": "object",
          "required": false,
          "description": "analysisDefinition object.",
          "http": {
            "source": "body"
          }
        },
        {
          "arg": "ctx",
          "type": "object",
          "http": {
            "source": "context"
          }
        }
      ],
      "returns": {
        "arg": "data",
        "type": [
          "ProAnalysis"
        ],
        "root": true
      },
      "description": "Run analysis by sending all the necessary information to the api (no persisted analysis settings involved)",
      "http": {
        "path": "/run",
        "verb": "post"
      }
    },
    "runAndExport": {
      "accepts": [
        {
          "arg": "pkProject",
          "type": "number",
          "required": true,
          "description": "Pk of the project.",
          "http": {
            "source": "query"
          }
        },
        {
          "arg": "analysisType",
          "type": "number",
          "required": true,
          "description": "Pk of the analysis type.",
          "http": {
            "source": "query"
          }
        },
        {
          "arg": "analysisDefinition",
          "type": "object",
          "required": true,
          "description": "analysisDefinition object.",
          "http": {
            "source": "body"
          }
        },
        {
          "arg": "fileType",
          "type": "string",
          "required": true,
          "description": "Expected file type. Options: 'json', 'csv'.",
          "http": {
            "source": "query"
          }
        },
        {
          "arg": "ctx",
          "type": "object",
          "http": {
            "source": "context"
          }
        }
      ],
      "returns": {
        "arg": "data",
        "type": [
          "ProAnalysis"
        ],
        "root": true
      },
      "description": "Run analysis by sending all the necessary information to the api (no persisted analysis settings involved)",
      "http": {
        "path": "/run-and-export",
        "verb": "post"
      }
    },
    "findPerIdAndVersionAndProject": {
      "accepts": [
        {
          "arg": "pkProject",
          "type": "number",
          "required": true,
          "description": "Pk of the project",
          "http": {
            "source": "query"
          }
        },
        {
          "arg": "pkEntity",
          "type": "number",
          "required": false,
          "description": "Pk of the visual",
          "http": {
            "source": "query"
          }
        },
        {
          "arg": "version",
          "type": "number",
          "required": false,
          "description": "Entity version",
          "http": {
            "source": "query"
          }
        },
        {
          "arg": "ctx",
          "type": "object",
          "http": {
            "source": "context"
          }
        }
      ],
      "returns": {
        "arg": "data",
        "type": [
          "ProVisual"
        ],
        "root": true
      },
      "description": "Find analysis of project, pk and version. If version is omitted, finds the latest version. If pkEntity omitted, finds all analyses of project, in latest version.",
      "http": {
        "path": "/find-per-id-and-project",
        "verb": "get"
      }
    },
    "bulkUpsert": {
      "accepts": [
        {
          "arg": "pkProject",
          "type": "number",
          "required": true,
          "description": "Project",
          "http": {
            "source": "query"
          }
        },
        {
          "arg": "data",
          "type": [
            "ProAnalysis"
          ],
          "required": true,
          "description": "Array ProAnalysis",
          "http": {
            "source": "body"
          }
        },
        {
          "arg": "ctx",
          "type": "object",
          "http": {
            "source": "context"
          }
        }
      ],
      "returns": {
        "arg": "data",
        "type": "ProAnalysis",
        "root": true
      },
      "description": "Creates or updates instances of ProAnalysis.",
      "http": {
        "path": "/bulk-upsert",
        "verb": "put"
      }
    },
    "bulkDelete": {
      "accepts": [
        {
          "arg": "pks",
          "type": [
            "number"
          ],
          "required": true,
          "description": "Array of Primary Key of DatDigitals",
          "http": {
            "source": "body"
          }
        },
        {
          "arg": "pkProject",
          "type": "number",
          "required": true,
          "description": "Project to which the ProAnalyses belong",
          "http": {
            "source": "query"
          }
        },
        {
          "arg": "ctx",
          "type": "object",
          "http": {
            "source": "context"
          }
        }
      ],
      "returns": {
        "arg": "data",
        "type": "boolean",
        "root": true
      },
      "description": "Deletes instances of ProAnalysis.",
      "http": {
        "path": "/bulk-delete",
        "verb": "put"
      }
    }
  }
}
