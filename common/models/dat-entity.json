{
  "name": "DatEntity",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "mixins": {
    "SetupRemoteMethods": {
      "ignoreACL": false,
      "disableAllExcept": []
    }
  },
  "properties": {
    "pk_entity": {
      "type": "number",
      "id": true,
      "generated": true
    },
    "entity_version": {
      "type": "number",
      "generated": true
    },
    "notes": {
      "type": "string",
      "required": false
    },
    "tmsp_creation": {
      "type": "string",
      "generated": true
    },
    "tmsp_last_modification": {
      "type": "string",
      "generated": true
    },
    "sys_period": {
      "type": "string",
      "generated": true
    },
    "fk_namespace": {
      "type": "number",
      "generated": true
    }
  },
  "relations":{
    "namespace": {
      "type": "belongsTo",
      "model": "DatNamespace",
      "foreignKey": "fk_namespace",
      "primaryKey": "pk_entity"
    }
  },
  "validations": [],
  "acls": [
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "DENY"
    }
  ],
  "methods": {}
}
