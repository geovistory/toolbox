{
  "name": "DfhProperty",
  "plural": "DfhProperties",
  "base": "DfhEntity",
  "idInjection": true,
  "options": {
    "validateUpsert": true,
    "postgresql": {
      "schema": "data_for_history",
      "table": "property"
    }
  },
  "mixins": {
    "SetupRemoteMethods": {
      "ignoreACL": false,
      "disableAllExcept": []
    }
  },
  "properties": {
    "dfh_pk_property": {
      "type": "number",
      "id": true,
      "required": true
    },
    "dfh_identifier_in_namespace": {
      "type": "string"
    },
    "dfh_has_domain": {
      "type": "number",
      "required": true
    },
    "dfh_has_range": {
      "type": "number",
      "required": true
    },
    "dfh_creation_time": {
      "type": "date"
    },
    "dfh_modification_time": {
      "type": "date"
    },
    "dfh_standard_label": {
      "type": "string"
    },
    "dfh_fk_property_of_origin": {
      "type": "number"
    },
    "dfh_domain_instances_min_quantifier": {
      "type": "number",
      "required": true
    },
    "dfh_domain_instances_max_quantifier": {
      "type": "number",
      "required": true
    },
    "dfh_range_instances_min_quantifier": {
      "type": "number",
      "required": true
    },
    "dfh_range_instances_max_quantifier": {
      "type": "number",
      "required": true
    }
  },
  "validations": [],
  "relations": {
    "labels": {
      "type": "hasMany",
      "model": "DfhLabel",
      "foreignKey": "dfh_fk_property"
    },
    "text_properties": {
      "type": "hasMany",
      "model": "DfhTextProperty",
      "foreignKey": "dfh_fk_property"
    },
    "domain_class": {
      "type": "belongsTo",
      "model": "DfhClass",
      "foreignKey": "dfh_has_domain"
    },
    "range_class": {
      "type": "belongsTo",
      "model": "DfhClass",
      "foreignKey": "dfh_has_range"
    },
    "property_profile_view": {
      "type": "hasMany",
      "model": "DfhPropertyProfileView",
      "foreignKey": "dfh_pk_property",
      "options": {
        "nestRemoting": false
      }
    },
    "ui_context_config": {
      "type": "hasMany",
      "model": "ComUiContextConfig",
      "foreignKey": "fk_property"
    }
  },
  "acls": [
    {
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "DENY",
      "property": "*"
    },
    {
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "ALLOW",
      "property": [
        "find",
        "findById",
        "__get__labels",
        "__get__text_properties",
        "propertyByDomainClass"
      ]
    }
  ],
  "methods": {
    "propertyByDomainClass": {
      "accepts": [
        {
          "arg": "dfh_pk_property",
          "type": "number",
          "required": true,
          "description": "dfh_pk_property",
          "http": {
            "source": "query"
          }
        }
      ],
      "returns": {
        "arg": "data",
        "type": [
          "DfhProperty"
        ],
        "root": true
      },
      "description": "Get all properties that are enabled and not removed from project, with labels, text_properties, domain_class, range_class.",
      "http": {
        "path": "/selected-pe-it-classes-of-profile",
        "verb": "get"
      }
    }
  }
}
