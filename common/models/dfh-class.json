{
  "name": "DfhClass",
  "plural": "DfhClasses",
  "base": "DfhEntity",
  "idInjection": true,
  "options": {
    "validateUpsert": true,
    "postgresql": {
      "schema": "data_for_history",
      "table": "class"
    }
  },
  "mixins": {
    "SetupRemoteMethods": {
      "ignoreACL": false,
      "disableAllExcept": []
    }
  },
  "properties": {
    "dfh_pk_class": {
      "type": "number",
      "id": true,
      "generated": true
    },
    "dfh_identifier_in_namespace": {
      "type": "string"
    },
    "dfh_standard_label": {
      "type": "string"
    }
  },
  "validations": [],
  "relations": {
    "class_profile_view": {
      "type": "hasMany",
      "model": "DfhClassProfileView",
      "foreignKey": "dfh_fk_class",
      "options": {
        "nestRemoting": false
      }
    },
    "labels": {
      "type": "hasMany",
      "model": "DfhLabel",
      "foreignKey": "dfh_fk_class",
      "options": {
        "nestRemoting": false
      }
    },
    "text_properties": {
      "type": "hasMany",
      "model": "DfhTextProperty",
      "foreignKey": "dfh_fk_class"
    },
    "ingoing_properties": {
      "type": "hasMany",
      "model": "DfhProperty",
      "foreignKey": "dfh_has_range",
      "primaryKey": "dfh_pk_class"
    },
    "outgoing_properties": {
      "type": "hasMany",
      "model": "DfhProperty",
      "foreignKey": "dfh_has_domain",
      "primaryKey": "dfh_pk_class"
    },
    "ui_context_configs": {
      "type": "hasMany",
      "model": "ComUiContextConfig",
      "foreignKey": "fk_class_for_property_set"
    },
    "property_sets": {
      "type": "hasMany",
      "model": "ComPropertySet",
      "foreignKey": "fk_class_for_property_set",
      "through": "ComUiContextConfig",
      "keyThrough": "fk_property_set"
    },
    "persistent_items": {
      "type": "hasMany",
      "model": "InfPersistentItem",
      "foreignKey": "fk_class",
      "primaryKey": "dfh_pk_class"
    }
  },
  "acls": [
    {
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "DENY",
      "property": "*"
    },
    {
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "ALLOW",
      "property": [
        "find",
        "findById",
        "__get__labels",
        "__get__text_properties",
        "selectedClassesOfProfile",
        "selectedPeItClassesOfProfile",
        "propertiesAndUiElements",
        "projectSettingsClassList"
      ]
    }
  ],
  "methods": {
    "selectedClassesOfProfile": {
      "accepts": [
        {
          "arg": "dfh_pk_profile",
          "type": "number",
          "required": false,
          "description": "Profile id",
          "http": {
            "source": "query"
          }
        }
      ],
      "returns": {
        "arg": "data",
        "type": [
          "DfhClass"
        ],
        "root": true
      },
      "description": "Get all classes that are selected by the profile of the specified primary-key.",
      "http": {
        "path": "/selected-classes-of-profile",
        "verb": "get"
      }
    },
    "selectedPeItClassesOfProfile": {
      "accepts": [
        {
          "arg": "dfh_pk_profile",
          "type": "number",
          "required": true,
          "description": "Profile id",
          "http": {
            "source": "query"
          }
        }
      ],
      "returns": {
        "arg": "data",
        "type": [
          "DfhClass"
        ],
        "root": true
      },
      "description": "Get all classes of persistent items that are selected and, if provided, filtered by the profile of the specified primary-key.",
      "http": {
        "path": "/selected-pe-it-classes-of-profile",
        "verb": "get"
      }
    },
    "propertiesAndUiElements": {
      "accepts": [
        {
          "arg": "pk_class",
          "type": "number",
          "required": true,
          "description": "Provide the dfh_pk_class to query one specific class",
          "http": {
            "source": "query"
          }
        },
        {
          "arg": "pk_ui_context",
          "type": "number",
          "required": true,
          "description": "Provide the pk_entity of the ui_context to query",
          "http": {
            "source": "query"
          }
        },
        {
          "arg": "pk_project",
          "type": "number",
          "required": false,
          "description": "Provide a pk_project for loading the settings of a project or leaf it blank to query the default settings.",
          "http": {
            "source": "query"
          }
        }
      ],
      "returns": {
        "arg": "data",
        "type": [
          "DfhClass"
        ],
        "root": true
      },
      "description": "Get ingoing and outgoing properties and ui elements of class",
      "http": {
        "path": "/properties-and-ui-elements",
        "verb": "get"
      }
    },
    "projectSettingsClassList": {
      "accepts": [
        {
          "arg": "pk_project",
          "type": "number",
          "required": true,
          "description": "The pk_project for whicht the list of classes is loaded.",
          "http": {
            "source": "query"
          }
        }
      ],
      "returns": {
        "arg": "data",
        "type": [
          "DfhClass"
        ],
        "root": true
      },
      "description": "Get a list of classes with info about the status they have int the project (used for the poject-settings > data-settings page).",
      "http": {
        "path": "/project-settings-class-list",
        "verb": "get"
      }
    }
  }
}
