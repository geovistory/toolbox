{
  "name": "DfhClass",
  "plural": "DfhClasses",
  "base": "DfhEntity",
  "idInjection": true,
  "options": {
    "validateUpsert": true,
    "postgresql": {
      "schema": "data_for_history",
      "table": "v_class"
    }
  },
  "mixins": {
    "SetupRemoteMethods": {
      "ignoreACL": false,
      "disableAllExcept": []
    }
  },
  "properties": {
    "dfh_pk_class": {
      "type": "number",
      "id": true,
      "generated": true
    },
    "dfh_identifier_in_namespace": {
      "type": "string"
    },
    "dfh_standard_label": {
      "type": "string"
    },
    "dfh_fk_system_type": {
      "type": "number"
    },
    "removed_from_api": {
      "type": "object"
    }
  },
  "validations": [],
  "relations": {
    "proj_rels": {
      "type": "hasMany",
      "model": "ProDfhClassProjRel",
      "primaryKey": "pk_entity",
      "foreignKey": "fk_entity"
    },
    "class_profile_view": {
      "type": "hasMany",
      "model": "DfhClassProfileView",
      "foreignKey": "dfh_fk_class",
      "options": {
        "nestRemoting": false
      }
    },
    "labels": {
      "type": "hasMany",
      "model": "DfhLabel",
      "foreignKey": "dfh_fk_class",
      "options": {
        "nestRemoting": false
      }
    },
    "text_properties": {
      "type": "hasMany",
      "model": "DfhTextProperty",
      "foreignKey": "dfh_fk_class"
    },
    "ingoing_properties": {
      "type": "hasMany",
      "model": "DfhProperty",
      "foreignKey": "dfh_has_range",
      "primaryKey": "dfh_pk_class"
    },
    "outgoing_properties": {
      "type": "hasMany",
      "model": "DfhProperty",
      "foreignKey": "dfh_has_domain",
      "primaryKey": "dfh_pk_class"
    },
    "class_field_configs": {
      "type": "hasMany",
      "model": "ProClassFieldConfig",
      "foreignKey": "fk_class_for_class_field"
    },
    "class_fields": {
      "type": "hasMany",
      "model": "SysClassField",
      "foreignKey": "fk_class_for_class_field",
      "through": "ProClassFieldConfig",
      "keyThrough": "fk_class_field"
    },
    "persistent_items": {
      "type": "hasMany",
      "model": "InfPersistentItem",
      "foreignKey": "fk_class",
      "primaryKey": "dfh_pk_class"
    }
  },
  "acls": [
    {
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "DENY",
      "property": "*"
    },
    {
      "principalType": "ROLE",
      "principalId": "$authenticated",
      "permission": "ALLOW",
      "property": [
        "find",
        "findById",
        "findComplex"
      ]
    },
    {
      "principalType": "ROLE",
      "principalId": "project_member",
      "permission": "ALLOW",
      "property": [
        "classesOfProjectProfiles"
      ]
    },
    {
      "principalType": "ROLE",
      "principalId": "system_admin",
      "permission": "ALLOW",
      "property": [
        "classesOfProfile"
      ]
    }
  ],
  "methods": {
    "classesOfProjectProfiles": {
      "accepts": [
        {
          "arg": "pkProject",
          "type": "number",
          "required": false,
          "description": "Project pk",
          "http": {
            "source": "query"
          }
        }
      ],
      "returns": {
        "arg": "data",
        "type": [
          "DfhClass"
        ],
        "root": true
      },
      "description": "Get all classes that are selected by at least one of the profiles used by the given project.",
      "http": {
        "path": "/classes-of-project-profiles",
        "verb": "get"
      }
    },
    "classesOfProfile": {
      "accepts": [
        {
          "arg": "dfh_pk_profile",
          "type": "number",
          "required": false,
          "description": "Profile id",
          "http": {
            "source": "query"
          }
        }
      ],
      "returns": {
        "arg": "data",
        "type": [
          "DfhClass"
        ],
        "root": true
      },
      "description": "Get all classes that are selected by the profile of the specified primary-key.",
      "http": {
        "path": "/selected-classes-of-profile",
        "verb": "get"
      }
    },
    "selectedPeItClassesOfProfile": {
      "accepts": [
        {
          "arg": "dfh_pk_profile",
          "type": "number",
          "required": true,
          "description": "Profile id",
          "http": {
            "source": "query"
          }
        }
      ],
      "returns": {
        "arg": "data",
        "type": [
          "DfhClass"
        ],
        "root": true
      },
      "description": "Get all classes of persistent items that are selected and, if provided, filtered by the profile of the specified primary-key.",
      "http": {
        "path": "/selected-pe-it-classes-of-profile",
        "verb": "get"
      }
    }
  }
}
