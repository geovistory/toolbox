{
  "name": "InfDigitalObject",
  "base": "PersistedModel",
  "idInjection": false,
  "options": {
    "validateUpsert": true,
    "postgresql": {
      "schema": "information",
      "table": "digital_object"
    }
  },
  "mixins": {
    "SetupRemoteMethods": {
      "ignoreACL": false,
      "disableAllExcept": []
    }
  },
  "properties": {
    "pk_entity": {
      "id": true,
      "type": "number",
      "generated": true
    },
    "entity_version": {
      "type": "number",
      "default": 1
    },
    "notes": {
      "type": "string",
      "required": false
    },
    "tmsp_creation": {
      "type": "string",
      "generated": true,
      "hidden": true
    },
    "tmsp_last_modification": {
      "type": "string",
      "generated": true,
      "hidden": true
    },
    "sys_period": {
      "type": "string",
      "generated": true,
      "hidden": true
    },
    "js_quill_data": {
      "type": "object",
      "required": true
    }
  },
  "validations": [],
  "relations": {
    "chunks": {
      "type": "hasMany",
      "model": "InfChunk",
      "foreignKey": "fk_digital_object",
      "primaryKey": "pk_entity"
    }
  },
  "acls": [
    {
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "DENY",
      "property": "*"
    },
    {
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "ALLOW",
      "property": [
        "find",
        "replaceOrCreate",
        "deleteById"
      ]
    }
  ],
  "methods": {}
}