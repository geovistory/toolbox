{
  "name": "InfEntityAssociation",
  "base": "InfEntity",
  "idInjection": false,
  "options": {
    "validateUpsert": true,
    "postgresql": {
      "schema": "information",
      "table": "v_entity_association"
    }
  },
  "mixins": {
    "SetupRemoteMethods": {
      "ignoreACL": false,
      "disableAllExcept": []
    }
  },
  "properties": {
    "fk_property": {
      "type": "number",
      "required": true
    },
    "fk_domain_entity": {
      "type": "number",
      "required": true
    },
    "fk_range_entity": {
      "type": "number",
      "required": true
    }
  },
  "validations": [],
  "relations": {
    "domain_pe_it": {
      "type": "belongsTo",
      "model": "InfPersistentItem",
      "foreignKey": "fk_domain_entity",
      "primaryKey": "pk_entity"
    },
    "range_pe_it": {
      "type": "belongsTo",
      "model": "InfPersistentItem",
      "foreignKey": "fk_range_entity",
      "primaryKey": "pk_entity"
    },
    "domain_chunk": {
      "type": "belongsTo",
      "model": "InfChunk",
      "foreignKey": "fk_domain_entity",
      "primaryKey": "pk_entity"
    },
    "range_chunk": {
      "type": "belongsTo",
      "model": "InfChunk",
      "foreignKey": "fk_range_entity",
      "primaryKey": "pk_entity"
    },
    "digital_object": {
      "type": "belongsTo",
      "model": "InfDigitalObject",
      "foreignKey": "fk_domain_entity",
      "primaryKey": "pk_entity"
    }
  },
  "acls": [
    {
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "DENY",
      "property": "*"
    },
    {
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "ALLOW",
      "property": [
        "find",
        "findComplex",
        "findOrCreateInfEntityAssociation",
        "nestedObject",
        "queryByParams",
        "mentioningsOfSection",
        "mentionings"
      ]
    }
  ],
  "methods": {
    "findOrCreateInfEntityAssociation": {
      "accepts": [
        {
          "arg": "projectId",
          "type": "number",
          "required": true,
          "description": "Id of the project",
          "http": {
            "source": "query"
          }
        },
        {
          "arg": "data",
          "type": "InfEntityAssociation",
          "required": true,
          "description": "data",
          "http": {
            "source": "body"
          }
        },
        {
          "arg": "ctx",
          "type": "object",
          "http": {
            "source": "context"
          }
        }
      ],
      "returns": {
        "arg": "data",
        "type": "InfEntityAssociation",
        "root": true
      },
      "description": "Find or create an information entity association.",
      "http": {
        "path": "/findOrCreate",
        "verb": "post"
      }
    },
    "nestedObject": {
      "accepts": [
        {
          "arg": "ofProject",
          "type": "boolean",
          "required": true,
          "description": "if true, finds project version. if false, finds repo version.",
          "http": {
            "source": "query"
          }
        },
        {
          "arg": "pkProject",
          "type": "number",
          "required": false,
          "description": "Primary Key of the Project. If provided and ofProject=false, makes a left join with project",
          "http": {
            "source": "query"
          }
        },
        {
          "arg": "pkEntity",
          "type": "number",
          "required": false,
          "description": "Primary Key of the entity association (pk_entity)",
          "http": {
            "source": "query"
          }
        },
        {
          "arg": "pkRangeEntity",
          "type": "number",
          "required": false,
          "description": "Foreign Key of the entity association pointing to the range entity (fk_range_entity)",
          "http": {
            "source": "query"
          }
        },
        {
          "arg": "pkDomainEntity",
          "type": "number",
          "required": false,
          "description": "Foreign Key of the entity association pointing to the domain entity (fk_domain_entity)",
          "http": {
            "source": "query"
          }
        },
        {
          "arg": "pkProperty",
          "type": "number",
          "required": false,
          "description": "Foreign Key of the entity association pointing to the property (fk_property)",
          "http": {
            "source": "query"
          }
        }
      ],
      "returns": {
        "arg": "data",
        "type": "InfEntityAssociation",
        "root": true
      },
      "description": "Get e nested object of all information about the found entity associations.",
      "http": {
        "path": "/nested-object",
        "verb": "get"
      }
    },
    "queryByParams": {
      "accepts": [
        {
          "arg": "ofProject",
          "type": "boolean",
          "required": true,
          "description": "if true, finds project version. if false, finds repo version.",
          "http": {
            "source": "query"
          }
        },
        {
          "arg": "pkProject",
          "type": "number",
          "required": false,
          "description": "Primary Key of the Project. If provided and ofProject=false, makes a left join with project",
          "http": {
            "source": "query"
          }
        },
        {
          "arg": "pkEntity",
          "type": "number",
          "required": false,
          "description": "Primary Key of the entity association (pk_entity)",
          "http": {
            "source": "query"
          }
        },
        {
          "arg": "pkRangeEntity",
          "type": "number",
          "required": false,
          "description": "Foreign Key of the entity association pointing to the range entity (fk_range_entity)",
          "http": {
            "source": "query"
          }
        },
        {
          "arg": "pkDomainEntity",
          "type": "number",
          "required": false,
          "description": "Foreign Key of the entity association pointing to the domain entity (fk_domain_entity)",
          "http": {
            "source": "query"
          }
        },
        {
          "arg": "pkProperty",
          "type": "number",
          "required": false,
          "description": "Foreign Key of the entity association pointing to the property (fk_property)",
          "http": {
            "source": "query"
          }
        }
      ],
      "returns": {
        "arg": "InfEntityAssociation",
        "type": "array",
        "root": true
      },
      "description": "Find entity associations by params.",
      "http": {
        "path": "/find-by-params",
        "verb": "get"
      }
    },
    "mentionings": {
      "accepts": [
        {
          "arg": "ofProject",
          "type": "boolean",
          "required": true,
          "description": "if true, finds project version. if false, finds repo version.",
          "http": {
            "source": "query"
          }
        },
        {
          "arg": "pkProject",
          "type": "number",
          "required": true,
          "description": "Primary Key of the Project. If provided and ofProject=false, makes a left join with project",
          "http": {
            "source": "query"
          }
        },
        {
          "arg": "pkRangeEntity",
          "type": "number",
          "required": false,
          "description": "Foreign Key (fk_range_entity) to the source/section/chunk/spot that mentiones (range of 'geovP2 – is mentioned in')",
          "http": {
            "source": "query"
          }
        },
        {
          "arg": "pkDomainEntity",
          "type": "number",
          "required": false,
          "description": "Foreign Key (fk_domain_entity) to the mentioned entity (domain of 'geovP2 – is mentioned in')",
          "http": {
            "source": "query"
          }
        },
        {
          "arg": "pkSource",
          "type": "number",
          "required": false,
          "description": "Primary Key of the entity association (pk_entity)",
          "http": {
            "source": "query"
          }
        },
        {
          "arg": "pkExpression",
          "type": "number",
          "required": false,
          "description": "Primary Key of the entity association (pk_entity)",
          "http": {
            "source": "query"
          }
        },
        {
          "arg": "pkChunk",
          "type": "number",
          "required": false,
          "description": "Primary Key of the entity association (pk_entity)",
          "http": {
            "source": "query"
          }
        }
      ],
      "returns": {
        "arg": "InfEntityAssociation",
        "type": "array",
        "root": true
      },
      "description": "Get mentionigs of a project and filter by domain or range of 'geovP2 – is mentioned in'. Returns also context information, if the range is a F2 Expression (section), geovC2 Chunk or geovC3 Spot.",
      "http": {
        "path": "/mentionings",
        "verb": "get"
      }
    },
    "mentioningsOfSection": {
      "accepts": [
        {
          "arg": "ofProject",
          "type": "boolean",
          "required": true,
          "description": "if true, finds project version. if false, finds repo version.",
          "http": {
            "source": "query"
          }
        },
        {
          "arg": "pkProject",
          "type": "number",
          "required": true,
          "description": "Primary Key of the Project. If provided and ofProject=false, makes a left join with project",
          "http": {
            "source": "query"
          }
        },
        {
          "arg": "pkEntity",
          "type": "number",
          "required": false,
          "description": "Primary Key of the entity association (pk_entity)",
          "http": {
            "source": "query"
          }
        }
      ],
      "returns": {
        "arg": "InfEntityAssociation",
        "type": "array",
        "root": true
      },
      "description": "Find mentionings of section (F2 Expression).",
      "http": {
        "path": "/mentionings-of-section",
        "verb": "get"
      }
    }
  }
}