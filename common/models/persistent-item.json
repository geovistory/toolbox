{
  "name": "PersistentItem",
  "base": "PersistedModel",
  "idInjection": false,
  "options": {
    "validateUpsert": true,
    "postgresql": {
      "schema": "geovistory",
      "table": "persistent_item"
    }
  },
  "mixins": {
    "SetupRemoteMethods": {
      "disableAllExcept": []
    }
  },
  "hidden": [],
  "properties": {
    "pk_persistent_item": {
      "type": "number",
      "id": true,
      "required": true
    },
    "id_importation_integer": {
      "type": "number"
    },
    "id_importation_varchar": {
      "type": "string"
    },
    "notes": {
      "type": "string"
    },
    "tmsp_creation": {
      "type": "date"
    },
    "tmsp_last_modification": {
      "type": "date"
    },
    "fk_system_type": {
      "type": "number"
    },
    "semkey_peit": {
      "type": "string"
    },
    "sys_period": {
      "type": "object"
    }
  },
  "validations": [],
  "relations": {},
  "acls": [
    {
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "ALLOW",
      "property": [
        "createItem",
        "find"
      ]
    }
  ],
  "methods": {
    "createItem": {
      "accepts": [
        {
          "arg": "projectId",
          "type": "number",
          "required": true,
          "description": "Id of project to associate the persistent item with.",
          "http": {
            "source": "query"
          }
        },
        {
          "arg": "data",
          "type": "PersistentItem",
          "required": true,
          "description": "Data used to create the new persistent item",
          "http": {
            "source": "body"
          }
        }
      ],
      "returns": {
        "arg": "success",
        "type": "boolean",
        "root": true
      },
      "http": {
        "path": "/create",
        "verb": "post"
      }
    }
  }
}
