{
  "name": "InformationRole",
  "base": "PersistedModel",
  "idInjection": false,
  "options": {
    "validateUpsert": true,
    "postgresql": {
      "schema": "information",
      "table": "role"
    }
  },
  "mixins": {
    "SetupRemoteMethods": {
      "disableAllExcept": [
        "find"
      ]
    }
  },
  "properties": {
    "pk_role": {
      "type": "number",
      "id": true,
      "required": true
    },
    "pk_entity": {
      "type": "number",
      "required": true
    },
    "fk_property": {
      "type": "string",
      "required": true
    },
    "fk_entity": {
      "type": "number",
      "required": true
    },
    "fk_temporal_entity": {
      "type": "number",
      "required": true
    },
    "notes": {
      "type": "string"
    }
  },
  "validations": [],
  "relations": {
    "persistent_item": {
      "type": "belongsTo",
      "model": "PersistentItem",
