      "primaryKey": "pk_entity"
    },
    "appellation": {
      "type": "belongsTo",
      "model": "Appellation",
      "foreignKey": "fk_entity",
      "primaryKey": "pk_entity"
    },
    "language": {
      "type": "belongsTo",
      "model": "InformationLanguage",
      "foreignKey": "fk_entity",
      "primaryKey": "pk_entity"
    },
    "temporal_entity": {
      "type": "belongsTo",
      "model": "TemporalEntity",
