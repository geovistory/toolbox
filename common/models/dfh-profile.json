{
  "name": "DfhProfile",
  "base": "DfhEntity",
  "idInjection": true,
  "options": {
    "validateUpsert": true,
    "postgresql": {
      "schema": "data_for_history",
      "table": "v_profile"
    }
  },
  "mixins": {
    "SetupRemoteMethods": {
      "ignoreACL": false,
      "disableAllExcept": []
    }
  },
  "properties": {
    "dfh_pk_profile": {
      "type": "number"
    },
    "dfh_profile_label_en": {
      "type": "string"
    },
    "dfh_profile_definition_en": {
      "type": "sting"
    },
    "dfh_owned_by_project": {
      "type": "number"
    },
    "dfh_project_label_en": {
      "type": "string"
    },
    "dfh_is_ongoing_forced_publication": {
      "type": "boolean"
    },
    "dfh_date_profile_published": {
      "type": "string"
    },
    "dfh_date_profile_deprecated": {
      "type": "string"
    }
  },
  "validations": [],
  "relations": {},
  "acls": [
    {
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "DENY",
      "property": "*"
    },
    {
      "principalType": "ROLE",
      "principalId": "$authenticated",
      "permission": "ALLOW",
      "property": [
        "updateFromOntoMe"
      ]
    },
    {
      "principalType": "ROLE",
      "principalId": "project_member",
      "permission": "ALLOW",
      "property": [
        "updateAndAddToProject"
      ]
    }
  ],
  "methods": {
    "updateFromOntoMe": {
      "accepts": [
        {
          "arg": "pkProfile",
          "type": "number",
          "required": true,
          "description": "OntoMe profile that should be added",
          "http": {
            "source": "query"
          }
        },
        {
          "arg": "requestedLanguage",
          "type": "string",
          "required": false,
          "description": "Language ISO Code for the preferred language of labels and descriptions to be loaded. Default language is 'en'.",
          "http": {
            "source": "query"
          }
        }
      ],
      "returns": {
        "arg": "data",
        "type": [
          "object"
        ],
        "root": true
      },
      "description": "Pulls profile data including classes and properties from OntoMe and updates profile data in geovistory.",
      "http": {
        "path": "/update-from-ontome",
        "verb": "get"
      }
    },
    "updateAndAddToProject": {
      "accepts": [
        {
          "arg": "pkProject",
          "type": "number",
          "required": true,
          "description": "Geovistory project to which the OntoMe profile should be added",
          "http": {
            "source": "query"
          }
        },
        {
          "arg": "pkProfile",
          "type": "number",
          "required": true,
          "description": "OntoMe profile that should be added",
          "http": {
            "source": "query"
          }
        },
        {
          "arg": "requestedLanguage",
          "type": "string",
          "required": false,
          "description": "Language ISO Code for the preferred language of labels and descriptions to be loaded. Default language is 'en'.",
          "http": {
            "source": "query"
          }
        }
      ],
      "returns": {
        "arg": "data",
        "type": [
          "object"
        ],
        "root": true
      },
      "description": "Pulls profile data including classes and properties from OntoMe andÂ adds profile to project.",
      "http": {
        "path": "/update-from-ontome-and-add-to-project",
        "verb": "get"
      }
    }
  }
}
