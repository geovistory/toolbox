{
  "name": "ComClassField",
  "base": "PersistedModel",
  "idInjection": false,
  "options": {
    "validateUpsert": true,
    "postgresql": {
      "schema": "commons",
      "table": "class_field"
    }
  },
  "mixins": {
    "SetupRemoteMethods": {
      "ignoreACL": false,
      "disableAllExcept": []
    }
  },
  "properties": {
    "pk_entity": {
      "type": "number",
      "id": true,
      "generated": true
    },
    "description": {
      "type": "string"
    },
    "label": {
      "type": "string"
    },
    "fk_system_type_ng_component": {
      "type": "number"
    },
    "used_table": {
      "type": "string"
    }
  },
  "validations": [],
  "relations": {
    "ui_context_configs": {
      "type": "hasMany",
      "model": "ComUiContextConfig",
      "foreignKey": "fk_class_field"
    },
    "classes": {
      "type": "hasMany",
      "model": "DfhClass",
      "foreignKey": "fk_class_field",
      "through": "ComUiContextConfig",
      "keyThrough": "fk_class_for_class_field"
    },
    "class_field_property_rel": {
      "type": "hasMany",
      "model": "ComClassFieldPropertyRel",
      "foreignKey": "fk_class_field"
    },
    "labels": {
      "type": "hasMany",
      "model": "ComLabel",
      "foreignKey": "fk_entity"
    }
  },
  "acls": [
    {
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "DENY",
      "property": "*"
    },
    {
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "ALLOW",
      "property": [
        "find",
        "findComplex"
      ]
    }
  ],
  "methods": {}
}
